{
  "name": "traction-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm-run-all -p serve:css serve",
    "serve": "vite",
    "build": "npm-run-all build:css build:js",
    "build:js": "vite build",
    "test:unit": "vitest run",
    "test:e2e:all": "npm-run-all -p start test:cypress:all",
    "test:e2e:one": "npm-run-all -p start test:cypress:one",
    "test:cypress:all": "cypress run",
    "test:cypress:one": "cypress open",
    "lint": "eslint . --ext .js,.vue,.cjs",
    "pretty": "prettier --check '**/*'",
    "test": "npm-run-all test:unit test:e2e:all",
    "serve:css": "postcss src/styles/tailwind.css -o src/styles/index.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/index.css --env production"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bootstrap-vue": "^2.22.0",
    "core-js": "^3.23.1",
    "csv-parse": "^5.1.0",
    "flush-promises": "^1.0.2",
    "global": "^4.4.0",
    "https-proxy-agent": "^2.2.2",
    "lodash-es": "^4.17.21",
    "postcss": "^7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vite": "^2.9.12",
    "vite-plugin-vue2": "^2.0.1",
    "vue": "^2.6.12",
    "vue-router": "^3.5.3",
    "vue-selecto": "^1.16.2",
    "vue-simple-spinner": "^1.2.10",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/eslint-parser": "7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^28.1.1",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "cypress": "^10.2.0",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7",
    "prettier": "2.5.1",
    "sass": "^1.52.3",
    "vitest": "^0.15.2",
    "vue-template-compiler": "^2.6.12"
  },
  "jest": {
    "roots": [
      "src/",
      "tests/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@tests/(.*)$": "<rootDir>/tests/$1"
    },
    "modulePaths": [
      "<rootDir>/tests/support/"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "^.+\\.(vue)$": "@vue/vue2-jest"
    },
    "transformIgnorePatterns": [
      "/!node_modules\\/lodash-es/"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testEnvironment": "jsdom",
    "setupFiles": [
      "dotenv/config"
    ]
  }
}
